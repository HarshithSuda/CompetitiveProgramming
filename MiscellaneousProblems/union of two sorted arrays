#include<bits/stdc++.h>
using namespace std;
void solve(){
	int n;
	cin>>n;
	int m;
	cin>>m;
	int arr[n];
	for(int i=0;i<n;i++){
		cin>>arr[i];
	}
	int arr1[m];
	for(int i=0;i<m;i++){
		cin>>arr1[i];
	}
	while(i<n && j<n){
		if(arr[i]>arr1[j]){
			cout<<arr1[j]<<" ";
			j++;
		}
		else if(arr[i]<arr1[j]){
			cout<<arr[i]<<" ";
			i++;
		}
		else{
			cout<<arr[i]<<" ";
			i++;
			j++;
		}
	}
	if(j<m){
		for(int i=j;j<m;j++){
			cout<<arr1[j]<<" ";
		}
	}
	else{
		for(int k=i;k<n;k++){
			cout<<arr[k]<<" ";
		}
	}
	return;
}
int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
#ifndef 
	freopen("input1.txt","r",stdin);
	freopen("output1.txt","w",stdout);
#endif
	solve();
	return 0; 
}